<div class="card">
  <div class="card-content">
    <span class="card-title"><%= title %></span>
    <div class="row">
      <% for item in items %>
        <div class="col s3">
          <%= render partial: 'partials/item_preview', locals: {item: item} %>
        </div>
      <% end %>
    </div>
  </div>
</div>
