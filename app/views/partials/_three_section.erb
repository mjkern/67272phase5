<div class="row">
  <div class="col s8">
    <div class="row">
      <div class="col s12">
        <%= render partial: "#{primary}" %>
      </div>
    </div>
    <% if !(secondary.nil?) %>
      <div class="row">
        <div class="col s12">
          <%= render partial: "#{secondary}" %>
        </div>
      </div>
    <% end %>
  </div>
  <div class="col s4">
    <%= render partial: "#{sidebar}" %>
  </div>
</div>
